double calc_deltaqq(double alpha_s_red, double Nf) {
   return 5.67 * alpha_s_red + (35.94 - 1.36 * Nf) * Sqr(alpha_s_red) +
          (164.14 - 25.77 * Nf + 0.259 * Sqr(Nf)) * std::pow(alpha_s_red, 3) +
          (39.34 - 220.9 * Nf + 9.685 * std::pow(Nf, 2) - 0.0205 * std::pow(Nf, 3)) *
             std::pow(alpha_s_red, 4);
}

